#!/bin/bash

# Get the list of files in the current directory
files=$(ls -a)

# Iterate over the list of files and display the part of the name after the first dash
for file in $files; do
  # Check if the file is hidden
  if [[ $file != .* ]]; then
    # Get the part of the name after the first dash
    part_after_dash=${file#*-}

    # Display the part of the name after the first dash
    echo "$part_after_dash"
  fi
done
